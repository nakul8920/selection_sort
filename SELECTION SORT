TOPIC 2 = SELECTION SORT.
def sort_list(list):
    for i in range(len(list)):
        min_index = i
        for j in range(i + 1, len(list)):
            if list[j] < list[min_index]:
                min_index = j
        list[i], list[min_index] = list[min_index], list[i]
my_list = [54,78,23,78,90,45]
sort_list(my_list)
print("Sorted list:", my_list)
